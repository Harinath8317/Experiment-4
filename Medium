#include<stdio.h>
int main ()
{
int n,i, largest, secondlargest;
printf("enter a number");
scanf("%d",&n);
 int arr[n];
 printf("enter a element");
 for (i=0;i<n;i++)
 {
 scanf("%d",&arr[i]);
 }
 largest=arr[0];
 secondlargest=arr[0];
 for(i=1;i<n;i++)
 {
 if(arr[i]>largest){
 secondlargest=largest;
 largest=arr[i];}
 else if (arr[i]>secondlargest &&arr[i]!=largest){
 secondlargest=arr[i];
 }
 {
 printf("the second largest number is:%d\n", secondlargest);
 }
 }
 }
